<style type="text/css">

/** {
    margin:0;
    padding:0;
}*/
body {
    background:#141414;
    position:relative;
    overflow:auto;
}
h1 {
    position:fixed;
    height:60px;
    width:100%;
    line-height:60px;
    font-size:1.75em;
    text-align:center; 
    color:#FFF;
}

#wrapper {
    float:left;
    margin:80px 20px 20px;
    margin-right:-99999em;
    position:relative;
}
.floatbox {
    float:left; 
    margin:0 20px 0 0;
}
.floatbox img {float:left;}

.caption {
    float:left; 
    width:150px;
    margin:0 0 0 20px;
    padding:10px;  
    background:#000;
    color:#FFF;
}
</style>



<div class="container-fluid">
  <div class="row-fluid">
    <div class="span2">
      pasta batman
    </div>
    <div class="span10">

    <% @photos.each do |photo| %>
        <div class="floatbox"> 
            <%= image_tag photo.source_url, size: "#{photo.width}x#{photo.height}" %>
        </div>
    <% end %>

    </div>
  </div>
</div>





</html>




